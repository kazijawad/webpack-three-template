{
    "root": true,
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 12
    },
    "extends": [
        "eslint:recommended",
        "plugin:import/errors",
        "plugin:import/warnings"
    ],
    "rules": {
        "indent": ["error", 4],
        "quotes": ["error", "single"],
        "max-len": 0
    }
}
